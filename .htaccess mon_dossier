########################################################################
#	Protection Youtube anti-hammeçonnage
#
<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteCond %{HTTP_REFERER} http(s)?://(www.)?.*youtube.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} http(s)?://(www.)?.*youtube-nocookie.com.*$ [NC]	
	RewriteCond %{REQUEST_URI} http(s)?://(www.)?.*youtube.com.*$ [NC,OR]
	RewriteCond %{REQUEST_URI} http(s)?://(www.)?.*youtube-nocookie.com.*$ [NC]
	RewriteRule .* - [F]
</IfModule>

########################################################################
#	Filtre GOOGLE YOUTUBE W3
#
<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteCond %{HTTP_REFERER} ^http(s)?://(fonts.)?.*gstatic\.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(jnn-pa.)?.*googleapis\.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(play.)?.*google\.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(www.)?.*google\.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(www.)?.*youtube\.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(www.)?.*youtube-nocookie\.com.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(www.)?.*youtube\.fr.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(www.)?.*youtube-nocookie\.fr.*$ [NC,OR]
	RewriteCond %{HTTP_REFERER} ^http(s)?://(www.)?.*w3\.org.*$ [NC]
	RewriteRule .* - [F]
</IfModule>

