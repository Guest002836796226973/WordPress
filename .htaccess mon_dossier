# BEGIN mon_fichier
<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteCond %{ENV:REDIRECT_STATUS} ^$
	RewriteRule ^index\.php(/(.*)|$) %{CONTEXT_PREFIX}/$2 [R=301,F]
	RewriteCond %{REQUEST_FILENAME} -f
	RewriteRule .? - [L]
	RewriteCond %{REQUEST_URI}::$1 ^(/.+)(.+)::\2$
	RewriteRule ^(.*) - [E=BASE:%1,NS]
	RewriteRule .? %{ENV:BASE}index.php [L,NS]
</IfModule>

########################################################################
#	Protection des dossiers de WordPress, thème et téléchargement (ligne préfixe # = bogue éditeur privé)
#
<IfModule mod_rewrite.c>
	RewriteEngine On
#	RewriteCond %{REQUEST_URI} /(__bbpress|__init|admin|export-personal-data|erase-personal-data|file|edit-comments|index|post|wp-comments-post|wp-tinymce)\.php [NC,OR]
#	RewriteCond %{REQUEST_URI} /(comments|content-none|content-page|content|footer|functions|header|index|page|search|sidebar|single)\.php [NC,OR]
	RewriteCond %{REQUEST_URI} /(/embed/) [NC,OR]
	RewriteCond %{REQUEST_URI} /(/2024/|/2025/|/2026/|/01/|/02/|/03/|/04/|/05/|/06/|/07/|/08/|/09/|/10/|/11/|/12/) [NC,OR]
